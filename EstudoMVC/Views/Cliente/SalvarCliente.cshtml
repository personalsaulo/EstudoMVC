@model EstudoMVC.Models.Cliente

@{
    /**/


    Layout = null;

}



<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SalvarCliente</title>
</head>
<body>
    <div>
        <fieldset>
            <legend>Cliente Salvo</legend>
            <div>
                <label for="Id">Id</label>
            </div>
            <div>
                <input type="text" value="@Model.Id" id="Id" name="Id" />
            </div>
            <div>
                <label for="Nome">Nome</label>

            </div>

            <div>

                <input type="text" value="@Model.Nome" id="Nome" name="Nome" />

            </div>

            <div>

                <label for="Email">Email</label>

            </div>

            <div>

                <input type="text" value="@Model.Email" id="Email" name="Email" />

            </div>

            <div>

                <label for="Telefone">Telefone</label>

            </div>

            <div>

                <input type="text" value="@Model.Telefone" id="Telefone" name="Telefone" />

            </div>

        </fieldset>

    </div>

</body>

</html>